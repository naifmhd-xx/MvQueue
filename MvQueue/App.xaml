<Application
    x:Class="MvQueue.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:MvQueue"
    xmlns:xaml="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:xaml1="http://schemas.microsoft.com/winfx/2006/xaml/presentation">

    <Application.Resources>
        <xaml:ResourceDictionary>
            <xaml1:ItemsPanelTemplate x:Key="TokensPanelTemplate">
                <xaml1:ItemsStackPanel Orientation="Vertical" />
            </xaml1:ItemsPanelTemplate>
        </xaml:ResourceDictionary>
    </Application.Resources>
</Application>